{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Hangfire": "Information"
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=;Database=;User Id=; Password=;",
    "HangfireConnection": "Server=;Database=;User Id=; Password=;"
  },
  "Umbraco": {
    "CMS": {
